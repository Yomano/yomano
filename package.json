{
  "name": "yomano",
  "version": "0.2.1",
  "description": "Project starter!",
  "scripts": {
    "prepublish": "npm run compile",
    "build": "npm run compile",
    "compile": "coffee -cbo lib src",
    "watch": "coffee -cwbo lib src",
    "postversion": "node -e \"x=require('child_process').execSync;v=x('grep \\'\"version\"\\' package.json|grep -Po \\'\\\\d+\\\\.\\\\d+\\\\.\\\\d+\\'').toString().slice(0,-1);x('sed -ri \\'s/[0-9]+\\\\.[0-9]+\\\\.[0-9]+(.+?!!BUMPME!!)/'+v+'\\\\1/I\\' app.js')\""
  },
  "author": "Gustavo Vargas <xgvargas@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "ejs": "^2.4.2",
    "globby": "^5.0.0",
    "inquirer": "^1.1.2",
    "minimatch": "^3.0.2",
    "mkdirp": "^0.5.1",
    "positive": "^1.0.9",
    "progress": "^1.1.8"
  },
  "preferGlobal": true,
  "bin": {
    "yomano": "./app.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Yomano/yomano.git"
  },
  "bugs": {
    "url": "https://github.com/Yomano/yomano/issues"
  },
  "homepage": "https://github.com/Yomano/yomano#yomano",
  "keywords": [
    "generator",
    "scaffold",
    "scaffolding",
    "boilerplate",
    "starter-code"
  ]
}
